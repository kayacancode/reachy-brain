# KayaCan Configuration
# Copy to ~/.kayacan/config.env and fill in your keys

# Robot connection
ROBOT_IP="192.168.23.66"
SSH_PASS="root"

# OpenAI (for Whisper STT)
OPENAI_API_KEY="sk-..."

# ElevenLabs (TTS)
ELEVENLABS_API_KEY="..."
ELEVENLABS_VOICE_ID="REDACTED_VOICE_ID"

# Honcho (memory - optional but recommended)
HONCHO_API_KEY="..."
HONCHO_WORKSPACE_ID="reachy-mini"

# OpenClaw/Clawdbot (Claude LLM)
# Set this to your Mac's IP on the network
CLAWDBOT_ENDPOINT="http://YOUR_MAC_IP:18789/v1/chat/completions"
CLAWDBOT_TOKEN="your-openclaw-token"
CLAWDBOT_MODEL="claude-sonnet-4-20250514"

# Feature flags (all default to "true")
ENABLE_FACE_RECOGNITION="true"
ENABLE_HONCHO="true"
ENABLE_TOOLS="true"
